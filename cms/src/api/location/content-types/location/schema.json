{
  "kind": "collectionType",
  "collectionName": "locations",
  "info": {
    "singularName": "location",
    "pluralName": "locations",
    "displayName": "Location"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "region": {
      "type": "enumeration",
      "enum": [
        "Bay of Kotor",
        "Budva Riviera",
        "Bar Riviera",
        "Ulcinj Riviera",
        "Tivat",
        "Herceg Novi",
        "Kotor",
        "Budva",
        "Bar",
        "Ulcinj"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "lat": {
      "type": "decimal",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "lng": {
      "type": "decimal",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "boats": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::boat.boat",
      "mappedBy": "home_marina",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    }
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  }
}
