{
  "kind": "collectionType",
  "collectionName": "boats",
  "info": {
    "singularName": "boat",
    "pluralName": "boats",
    "displayName": "Boat"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images"
      ]
    },
    "boat_type": {
      "type": "enumeration",
      "default": "Motorboat",
      "enum": [
        "RIB",
        "Motorboat",
        "Speedboat",
        "Sailboat",
        "Yacht",
        "Catamaran",
        "Fishing boat",
        "Jet ski",
        "Other"
      ]
    },
    "capacity": {
      "type": "integer",
      "required": true,
      "min": 1
    },
    "length_m": {
      "type": "decimal"
    },
    "engine_hp": {
      "type": "integer"
    },
    "license_required": {
      "type": "boolean",
      "default": false
    },
    "skipper_available": {
      "type": "boolean",
      "default": true
    },
    "skipper_price_per_hour": {
      "type": "decimal"
    },
    "skipper_price_per_day": {
      "type": "decimal"
    },
    "currency": {
      "type": "enumeration",
      "default": "EUR",
      "enum": [
        "EUR"
      ]
    },
    "brand": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::brand.brand",
      "inversedBy": "boats"
    },
    "purposes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::purpose.purpose",
      "inversedBy": "boats"
    },
    "home_marina": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::location.location",
      "inversedBy": "boats"
    },
    "extras": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::extra.extra"
    },
    "rate_plans": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::rate-plan.rate-plan",
      "mappedBy": "boat"
    },
    "booking_requests": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::booking-request.booking-request",
      "mappedBy": "boat"
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
}
